    <eddyfi:SvgIcon x:Name="Icon3" Source="./Resources/Dice3.svg" />
    <eddyfi:SvgIcon x:Name="Icon4" Source="./Resources/Dice4.svg" />
    <eddyfi:SvgIcon x:Name="Icon5" Source="./Resources/Dice5.svg" />
    <eddyfi:SvgIcon x:Name="Icon6" Source="./Resources/Dice6.svg" />

  </Grid>
